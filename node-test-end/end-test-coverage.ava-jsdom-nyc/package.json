{
	"name": "end-test.ava-jsdom",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test": "ava --verbose",
		"coverage": "nyc ava --verbose"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"ava": "^0.25.0",
		"babel-preset-env": "^1.7.0",
		"babel-register": "^6.26.0",
		"browser-env": "^3.2.5",
		"nyc": "^15.1.0"
	},
	"ava": {
		"files": [
			"./src-test/**/*.test.js",
			"./src-end-test/**/*.test.js"
		],
		"source": [
			"./src/**/*.{js}"
		],
		"concurrency": 5,
		"failFast": true,
		"require": [
			"babel-register",
			"./src-end-test/helpers/setup-browser-env.js"
		]
	},
	"babel": {
		"babelrc": false,
		"presets": [
			"env"
		],
		"env": {
			"development": {
				"sourceMaps": "inline"
			}
		}
	}
}